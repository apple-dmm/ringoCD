<%= render 'admin/sidemenu' %>

<h2 class="text">歌手名の編集・削除</h2>
<div class="border"></div>
<div class="blank"></div>



  <%= form_for @artist, url: admin_artist_path do |f| %>

  <%= f.label :歌手名 %>
  <%= f.text_field :name, autofocus: true, id:"artists" %>

  <%= f.submit '保存する', class: "btn btn-primary" %>

  <%= link_to '削除する', admin_artist_path, method: :delete , class: 'btn btn-primary' %>

  <% end %>


